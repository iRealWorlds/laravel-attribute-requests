{
    "name": "irealworlds/laravel-attribute-requests",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ireal\\AttributeRequests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ireal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Ireal\\AttributeRequests\\Providers\\AttributeRequestServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Marin Calin",
            "email": "marincalin2551@hotmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^10",
        "illuminate/http": "^10",
        "illuminate/validation": "^10",
        "illuminate/contracts": "^10"
    },
    "require-dev": {
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-faker": "^2.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
